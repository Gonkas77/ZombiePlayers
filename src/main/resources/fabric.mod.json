	"authors": [
		"Gonkas"
	],
	"contact": {
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/zombieplayers/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"me.gonkas.zombieplayers.ZombiePlayers"
		],
		"client": [
			"me.gonkas.zombieplayers.ZombiePlayersClient"
		],
		"fabric-datagen": [
			"me.gonkas.zombieplayers.ZombiePlayersDataGenerator"
		]
	},
	"mixins": [
		"zombieplayers.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.2",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
